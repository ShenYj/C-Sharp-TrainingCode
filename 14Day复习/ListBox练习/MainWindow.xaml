        <ListBox Name="MyListBox" HorizontalAlignment="Left" Height="400" Margin="0,0,0,0" VerticalAlignment="Top" Width="760" Loaded="MyListBox_Loaded">
            <!--<local:DeviceListItem></local:DeviceListItem>-->
        </ListBox>
    </Grid>
</Window>
